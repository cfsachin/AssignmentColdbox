<cfoutput><div class="jumbotron">	<div class="row" id="rowloginform">		<div id="loader" style="display:none"></div>		<form name="loginform" id="loginform" action="#cgi.SCRIPT_NAME#?event=#Event.getValue('xehLogin')#" method="post" class="form-signin">			<h3 class="form-signin-heading">Welcome Back! Please Sign In</h3>			<h5><div id="cbox_messagebox_error" class="cbox_messagebox_error"></div></h5>			<hr class="colorgraph"><br>			<input type="hidden" name="event" id="event" value="#Event.getValue('xehLogin')#">		  	<input class="form-control" name="username" type="text" placeholder="Username" id="username" value="#Event.getValue("username","")#">		  	<input class="form-control" name="password" type="password" id="password" placeholder="Password" />	  		<button class="btn btn-lg btn-primary btn-block disabled" id="Submit" name="Submit" value="Login" type="button">Login</button>		</form>	</div>	<div class="row" id="rowlogoutform" style="display:none">		<form name="logoutform" id="logoutform" action="#cgi.SCRIPT_NAME#?event=#Event.getValue('xehLogout')#" method="post" class="form-signin">			<h3 class="form-signin-heading">Welcome Back! <span id="loggedUsername"></span></h3>			<h3 class="form-signin-heading">You have logged in successfully.</h3>			<input type="hidden" name="event" id="event" value="#Event.getValue('xehLogout')#">	  		<button class="btn btn-lg btn-primary btn-block" id="Logout" name="Logout" value="Logout" type="button">Logout</button>		</form>	</div></div></cfoutput>